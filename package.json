{
  "name": "markdown-to-snippet",
  "version": "0.1.0",
  "description": "Write editor snippets with markdown that also serves as documentation",
  "type": "module",
  "bin": "./lib/cli.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "prepare": "husky install && npm run build",
    "commit": "cz",
    "lint": "eslint .",
    "lint:ci": "NODE_ENV=production eslint .",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZachGawlik/markdown-to-snippet.git"
  },
  "keywords": [
    "vscode",
    "snippets",
    "vscode-snippets",
    "remark"
  ],
  "author": "Zach Gawlik <zachgawlik@gmail.com> (https://zachgawlik.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZachGawlik/markdown-to-snippet/issues"
  },
  "homepage": "https://github.com/ZachGawlik/markdown-to-snippet#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.2.2",
    "@types/lodash-es": "^4.17.6",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.11.9",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "chalk-template": "^0.4.0",
    "commander": "^9.2.0",
    "jest": "^29.2.2",
    "lodash-es": "^4.17.21",
    "log-symbols": "^5.1.0",
    "mdast-util-to-string": "^3.1.0",
    "mdast-zone": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2",
    "unist-util-select": "^4.0.1",
    "unist-util-visit": "^4.1.1"
  }
}
